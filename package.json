{
  "name": "trpc-uwebsockets",
  "description": "tRPC adapter for uWebSockets.js server",
  "version": "11.5.0",
  "main": "./dist/index.js",
  "type": "commonjs",
  "types": "./types/index.d.ts",
  "engines": {
    "node": "^22.11.0"
  },
  "scripts": {
    "start": "jest --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:publish": "yarn test && yarn build && yarn pack",
    "bump-version": "node ./bumpVersion.js && yarn install && yarn test",
    "publish:main": "yarn npm publish",
    "publish:canary": "yarn npm publish --tag canary",
    "prepack": "yarn build",
    "prepublish": "node ./checkVersion.js && yarn clean && yarn prettier && yarn lint && yarn test",
    "clean": "rimraf -rf ./dist && rimraf -rf ./types && rimraf -rf ./coverage && rimraf -rf ./package.tgz",
    "format": "yarn clean && yarn prettier && yarn lint",
    "lint": "eslint src/**/*.ts --fix",
    "lint:dry": "eslint src/**/*.ts",
    "prettier": "prettier src/**/*.ts --config .prettierrc --write",
    "prettier:dry": "prettier src/**/*.ts --config .prettierrc"
  },
  "dependencies": {
    "@trpc/server": "11.5.0"
  },
  "peerDependencies": {
    "uWebSockets.js": "*"
  },
  "peerDependenciesMeta": {
    "uWebSockets.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@trpc/client": "11.5.0",
    "@types/node": "^20.11.5",
    "@types/node-fetch": "^2.6.12",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "abort-controller": "^3.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "event-source-polyfill": "^1.0.31",
    "jsdom": "^26.0.0",
    "node-fetch": "3.3.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.51.0",
    "vite": "^6.2.5",
    "vitest": "^3.1.1",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "license": "MIT",
  "keywords": [
    "trpc",
    "uwebsockets",
    "trpc adapter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/romanzy313/trpc-uwebsockets.git"
  },
  "author": "Roman Volovoy",
  "packageManager": "yarn@4.8.1"
}
